def process_info(pid):
    info = process.info(sys.get_pid() if pid == -1 else pid)
    for key, value in info.items():
        if key == "cmd":
            print("cmd_args=")
            for nested_value in value.split(" "):
                print("\t- {}".format(nested_value))
        elif key == "environ":
             print("env_variables=")
             for nested_value in value.split(","):
                print("\t- {}".format(nested_value))
        else:
            print("{}={}".format(key, value))

time.sleep(1)
