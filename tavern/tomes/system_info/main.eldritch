def main():
    print("Operating System Information:")
    pprint(sys.get_os())
    print()
    print("hostname: ", sys.hostname())
    print()
    print("User Information:")
    pprint(sys.get_user())
    print()
    print("System time:")
    print(time.format_to_readable(time.now(), "%Y-%m-%d %H:%M:%S"))
    print()
    print("Network information:")
    pprint(sys.get_ip())

main()